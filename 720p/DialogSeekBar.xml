				<control type="label">
					<include>Objects_OSDSeekForward</include>
					<label>$LOCALIZE[31706] 16X</label>
					<visible>Player.Forwarding16x</visible>
				</control>
				<control type="label">
					<include>Objects_OSDSeekForward</include>
					<label>$LOCALIZE[31706] 32X</label>
